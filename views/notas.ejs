    <script>
        function validarInicioSesion() {
            // ObtÃ©n los valores de nombre y DNI
            var nombre = document.getElementById('nombre').value;
            var dni = document.getElementById('dni').value;

            // Validar si nombre o DNI estÃ¡n vacÃ­os
            if (!nombre || !dni) {
                // Mostrar alerta de error con SweetAlert2
                Swal.fire({
                    icon: 'error',
                    title: 'Error',
                    text: 'Por favor, ingresa tanto el nombre como el DNI.'
                });
                return false; // Evitar el envÃ­o del formulario
            }

            // Si la validaciÃ³n es exitosa, el formulario se enviarÃ¡ normalmente
            return true;
        }
    </script>




